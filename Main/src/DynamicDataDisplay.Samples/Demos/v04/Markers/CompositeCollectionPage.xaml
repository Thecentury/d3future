<Page x:Class="Microsoft.Research.DynamicDataDisplay.Samples.Demos.v04.Markers.CompositeCollectionPage"
	  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
	  xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
	  mc:Ignorable="d" 
	  d:DesignHeight="300" d:DesignWidth="300"
	  xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
	  Title="CompositeCollectionPage"
	  Loaded="Page_Loaded"
	  >

	<Grid>
		<d3:ChartPlotter Name="plotter">
			<d3:MarkerChart Name="chart">
				<d3:EllipseMarker MarkerWidth="5" MarkerHeight="5" MarkerFill="Blue"/>
			</d3:MarkerChart>
		</d3:ChartPlotter>
	</Grid>
</Page>
